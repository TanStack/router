    "@angular/platform-browser-dynamic": ">=19.0.0",
    "@tanstack/history": "workspace:*",
    "@tanstack/router-core": "workspace:*",
    "@tanstack/router-devtools-core": "workspace:*",
    "jsesc": "^3.0.2",
    "rxjs": "^7.8.0",
    "solid-js": "^1.9.5",
    "tiny-invariant": "^1.3.3",
    "tiny-warning": "^1.0.3"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "1.15.1-beta.3",
    "@analogjs/vitest-angular": "1.15.1-beta.3",
    "ng-packagr": "^19.2.0",
    "premove": "^4.0.0"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/fesm2022/tanstack-angular-store.mjs",  
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
