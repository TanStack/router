    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true
