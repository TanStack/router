{
  "name": "tanstack-router-react-example-with-trpc",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently 'pnpm run dev:server' 'pnpm run dev:client'",
    "dev:client": "vite dev",
    "dev:server": "tsx src/server/server.ts --watch",
    "build": "concurrently 'pnpm run build:server' 'pnpm run build:client'",
    "build:client": "vite build && tsc --noEmit",
    "build:server": "tsup",
    "start": "NODE_ENV=production node dist/server/server.js"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.122.0",
    "@tanstack/react-router-devtools": "^1.122.0",
    "@tanstack/router-plugin": "^1.122.0",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "concurrently": "^8.2.2",
    "express": "^4.21.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redaxios": "^0.5.1",
    "postcss": "^8.5.1",
    "autoprefixer": "^10.4.20",
    "tailwindcss": "^3.4.17",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "tsx": "^4.20.3",
    "tsup": "^8.5.0",
    "vite": "^6.3.5"
  }
}
