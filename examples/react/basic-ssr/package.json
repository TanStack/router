  },
  "devDependencies": {
    "@types/jsesc": "^3.0.1",
    "@vitejs/plugin-react": "^2.2.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "jsesc": "^3.0.2",
    "serve-static": "^1.15.0",
    "vite": "^3.2.3"
  }
}
